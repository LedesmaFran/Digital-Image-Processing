    "import cv2\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 59,
   "metadata": {},
   "outputs": [],
   "source": [
    "line = cv2.imread('linea.png')\n",
    "gray = cv2.cvtColor(line, cv2.COLOR_BGR2GRAY).astype(np.uint8)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [],
   "source": [
    "with open(\"line.mem\", \"x\") as f:\n",
    "    for row in gray:\n",
    "        for pixel in row:\n",
    "            f.write(hex(pixel)[2:] + '\\n')"
   ]
  }
